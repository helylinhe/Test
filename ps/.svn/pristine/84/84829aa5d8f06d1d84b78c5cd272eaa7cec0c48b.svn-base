html, body, ul, li, ol, dl, dd, dt, p, h1, h2, h3, h4, h5, h6, form, fieldset, legend, img 
{ margin:0; padding:0; }

ul,ol{
	list-style:none;
}
.lk_height{
	width:100%;
	height:auto;
	padding-top:15px;
	padding-left:15px;
}

div.lk_height > div > span {
	/*margin-left:400px;  */
	font-size:16px;
	font-family:"微软雅黑";
}

.lk_line{
	width:1600px
	height:auto;
	/*border:1px solid red;*/
	/*margin:80px 80px;*/
}

.lk_line p{
	margin-left:50px;
}

em {
	font-size:16px;
	font-family:"微软雅黑";
	
}

.lk_line div {
	
}

.lk_content > div{
