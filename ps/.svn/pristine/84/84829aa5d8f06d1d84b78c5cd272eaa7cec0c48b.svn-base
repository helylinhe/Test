}
.fright{
	float:right;
	margin:0px;
}

.active{
	position: relative;
border-color: #f4b39d;
border-bottom: solid 1px #f4b39d;
background: #FBEC88;
}

.sort{
	line-height: 31px;
	padding: 0 15px 0 20px;
	text-decoration: none;
}

.col{
	float: left;
margin-left: -1px;
border: 1px solid #ffcc66;
border-top: 0;
border-bottom: 0;
-webkit-box-shadow: 2px 0 2px -1px rgba(213,213,213,0.4);
-moz-box-shadow: 2px 0 2px -1px rgba(213,213,213,0.4);
box-shadow: 2px 0 2px -1px rgba(213,213,213,0.4);
}


.lk_size{
	font-size:20px;
	background-color:#FBEC88;
}

.lk_smallsize{
	font-size:15px;
}

.lk_bg{
	font-size:15px;
	font-family:'sans-serif';
	background-color:#FBEC88;
}

.map_rect1, .map_rect2, .map_rect3, .map_rect4{
	position: absolute;
    left: 40px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    padding: 3px;
    color: white;
    width: 10px;
    height: 10px;
    cursor: pointer;
    line-height: 10px;
    text-align: center;
}
.map_rect1{
	top:10px;
	background: #67FF03;
}
.map_rect2{
	top:30px;
	background: #a1a3a6;
}
.map_rect3{
	top:50px;
	background: #ffe600;
}
.map_rect4{
	top:70px;
	background: #d71345;
}

.map_text1, .map_text2, .map_text3, .map_text4{
	position: absolute;
    left: 60px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    padding: 3px;
    color: white;

    cursor: pointer;
    line-height: 10px;
    text-align: center;
	color:black;
	font-size:14px;
}
.map_text1{
	top:10px;
}
.map_text2{
	top:30px;
}
.map_text3{
	top:50px;
}
.map_text4{
	top:70px;
}