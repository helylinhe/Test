
* {
}
.bgcolor {
    background: none repeat scroll 0 0 #F9F9F9;
}
.bgTool {
    background: none repeat scroll 0 0 #EFEFEF;
}
.left {
    float: left;
}
.right {
    float: right;
}
.clear {
    clear: both;
}
.hidden {
    visibility: hidden;
}
.visible {
    visibility: visible;
}
.tip-info {
    background: none repeat scroll 0 0 #FFFEE6;
    color: #8F5700;
    padding: 12px;
}
.tip-icon {
    float: left;
    height: 16px;
    width: 24px;
}
.edit-table {
    font-size: 12px;
    line-height: 25px;
    text-align: right;
    width: 95%;
}
.edit-table input {
    color: #033F5B;
    width: 100%;
}
.view-title {
    border-bottom: 3px solid #EFEFEF;
    color: #416AA3;
    font-size: 16px;
    margin: 10px;
}
.view-table {
    font-size: 12px;
    line-height: 25px;
    text-align: right;
    width: 90%;
}
.view-table input {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: transparent;
    border-color: -moz-use-text-color -moz-use-text-color #EEEEEE;
    border-image: none;
    border-style: none none solid;
    border-width: 0 0 1px;
    width: 100%;
}
.view-title input {
    background-color: transparent;
    border: 0 none;
}
.easyui-accordion ul {
    margin-top: 5px;
}
.easyui-accordion ul li {
    cursor: pointer;
    display: block;
    height: auto;
    margin: 5px;
}
.easyui-accordion ul li a {
    display: block;
    height: auto;
    padding-left: 2px;
}
.view-panel {
    margin-left: 30px;
    margin-right: 30px;
}
.sign {
    background: url("../image/sign.png") no-repeat scroll 0 0 transparent;
    display: inline-block;
    float: left;
    height: 49px;
    margin-left: 20px;
    margin-top: 10px;
    width: 50px;
}
