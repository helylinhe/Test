$(function() {

	/***************************************************************************
	 * 获取基本信息 inpatiSerialNumber patiSerial inpatiNumber
	 * 
	 */
	var departmentValue = "";
	var patiSerialValue = $($('#patiSerial', parent.document)).val();
	var inpatiNumberValue = $($('#inpatiNumber', parent.document)).val();
