		// 处理延期情况
		var delayFlupDate = $("#delayFlupDate").val();
		var toDate = $("#toDate").val();
		var flag = compareTime(toDate, delayFlupDate);
		if (!flag) {
			$.messager.alert('提示', '延期日期不合法', 'info');
			return false;
		} else {
			$.messager.confirm('确认对话框', '是否延期随访？', function(r) {
